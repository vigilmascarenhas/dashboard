# API Keys for Dashboard Application
# Note: In Vite projects, environment variables must be prefixed with VITE_ to be accessible in the browser

# Alpha Vantage API Key for stock market data
VITE_ALPHAVANTAGE_API_KEY=your_alphavantage_api_key_here

# BrAPI API Key for Brazilian stock market data
VITE_BRAPI_API_KEY=your_brapi_api_key_here

# Finnhub API Key for US stock market data
VITE_FINNHUB_API_KEY=your_finnhub_api_key_here
